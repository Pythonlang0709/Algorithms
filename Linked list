class Node:
	def __init__(self,data=None):
		self.data = data
		self.next = None
class linkedlist:
	def __init__(self):
		self.head = None
	def printele(self):
		printval = self.head
		while printval is not None:
			print(printval.data)
			printval = printval.next
			
list1 = linkedlist()
list1.head = Node(0)
e2 = Node(1)
e3 = Node(2)
list1.head.next = e2
e2.next = e3
list1.printele()
print(e3.next)


