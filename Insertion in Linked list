class Node:
	def __init__(self,data=None):
		self.data = data
		self.next = None
class linkedlist:
	def __init__(self):
		self.head = None
	def printelements(self):
		printval = self.head
		while printval:
			print(printval.data,end=" ")
			printval = printval.next
	def front(self,data):
			newnode = Node(data)
			newnode.next = self.head
			self.head = newnode
	def end(self,data):
		newnode = Node(data)
		if self.head is None:
			self.head = newnode
		else:
			laste = self.head
			while (laste.next):
				laste = laste.next
			laste.next = newnode
	def insertnode(self,prev,data):
		if prev is None:
			print("The Node is not in the Linkedlist")
		else:
			newnode = Node(data)
			newnode.next = prev.next
			prev.next = newnode
		
		
list1 = linkedlist()
list1.head = Node(0)
e2 = Node(1)
e3 = Node(2)
list1.head.next = e2
e2.next = e3
list1.front("Whole Numbers :")
list1.end(".....")
list1.insertnode(list1.head,3)
list1.printelements()
