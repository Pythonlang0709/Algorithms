t = int(input())
for i in range(t):
    n,k = map(int,input().split())
    w = list(map(int,input().split()))
    rounds = 1
    lifted = 0
    for j in range(n):
        if w[j]>k:
            rounds = -1
            break
        lifted+=w[j]
        if lifted>k:
            lifted=w[j]
            rounds+=1
    
    print(rounds)            
        
