arr = [7,9,5,4,3,3,1]
stack = []
for i in arr:
    if len(stack)>0:
        if stack[-1]<i:
            stack.pop()
            stack.append(i)
        else:
            stack.append(i)
    else:
                 stack.append(i)
print(stack)                 
print(len(stack))                 
            
        
