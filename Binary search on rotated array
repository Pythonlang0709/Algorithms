arr = [5,6,7,8,9,10,1,2,3,4]
s = 0
e = len(arr)-1
ans = 8
f = arr[0]
while s<=e:
    mid = (s+e)//2
    if arr[mid] == ans:
        print("True",mid)
        break
    elif arr[mid]>=f:
        if ans<f or ans>arr[mid]:
            s = mid+1
        else:
            e = mid-1
    else:
        if ans>f or ans<arr[mid]:
            e = mid-1
        else:
            s = mid+1
else:
        print("False")   
        
           
